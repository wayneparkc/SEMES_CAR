<Window x:Class="SEMES_CAR_GUI.ResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SEMES_CAR_GUI"
        mc:Ignorable="d"
        Title="Result Window" Height="700" Width="500">
    <Grid>
        <Image x:Name="carType" Margin="40,194,40,194" Stretch="Uniform" Source="/assets/sedan.png"/>
        <Image x:Name="engine" Margin="8,162,362,422" Stretch="Uniform" Source="/assets/gm.png"/>
        <Image x:Name="breakSystem" Width="130" Height="100" Margin="185,162,185,422" Stretch="Uniform" Source="/assets/continental.png"/>
        <Image x:Name="steerSystem" Width="130" Height="100" Margin="359,162,11,422" Stretch="Uniform" Source="/assets/mobis.png"/>
        <TextBox HorizontalAlignment="Left" Height="20" Margin="31,156,0,0" TextWrapping="Wrap" Text="Engine" VerticalAlignment="Top" Width="84" BorderBrush="{x:Null}" FontWeight="Bold" TextAlignment="Center"/>
        <TextBox HorizontalAlignment="Center" Height="20" Margin="0,156,0,0" TextWrapping="Wrap" Text="BreakSystem" VerticalAlignment="Top" Width="84" BorderBrush="{x:Null}" FontWeight="Bold" TextAlignment="Center"/>
        <TextBox HorizontalAlignment="Left" Height="20" Margin="381,156,0,0" TextWrapping="Wrap" Text="SteerSystem" VerticalAlignment="Top" Width="86" BorderBrush="{x:Null}" FontWeight="Bold" TextAlignment="Center"/>
        <Button x:Name="RunButton" Content="RUN" HorizontalAlignment="Left" Height="40" Margin="129,503,0,0" VerticalAlignment="Top" Width="110" Background="#FF182869" Foreground="White" BorderBrush="{x:Null}" FontWeight="Bold">
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="5"/>
                </Style>
            </Button.Resources>
        </Button>
        <Button x:Name="TestButton" Content="TEST" HorizontalAlignment="Left" Height="40" Margin="264,503,0,0" VerticalAlignment="Top" Width="110" Background="#FF182869" Foreground="White" BorderBrush="{x:Null}" FontWeight="Bold">
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="5"/>
                </Style>
            </Button.Resources>
        </Button>
    </Grid>
</Window>
